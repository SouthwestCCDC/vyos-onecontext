# Simple router configuration
# Basic single-interface router with minimal features

# Identity
HOSTNAME="simple-router"
SSH_PUBLIC_KEY="ssh-ed25519 AAAAC3NzaC1lZDI1NTE5AAAAIExampleKey123456789abcdefghijklmnopqrstuvwxyz admin@simple-router"

# Operational mode
ONECONTEXT_MODE="stateless"

# Single interface with gateway
ETH0_IP="192.168.1.1"
ETH0_MASK="255.255.255.0"
ETH0_GATEWAY="192.168.1.254"
ETH0_DNS="8.8.8.8"
